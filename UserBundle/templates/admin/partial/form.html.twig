{{ form_start(form) }}
{{ form_widget(form) }}
<div class="mb-3 row">
    <div class="col-sm-2"></div>
    <div class="col-sm-10">
        {% if user.id is null %}
            <input type="submit" value="Update" class="btn btn-primary" />
            <a href="{{ path('nines_user_admin_index') }}" class="btn btn-warning"> Cancel </a>
        {% else %}
            <input type="submit" value="Update" class="btn btn-primary" />
            <a href="{{ path('nines_user_admin_show', { id: user.id }) }}" class="btn btn-warning"> Cancel </a>
        {% endif %}
    </div>
</div>
{{ form_end(form) }}