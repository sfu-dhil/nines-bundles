{{ form_start(form) }}
{{ form_widget(form) }}
<div class="mb-3 row">
    <div class="col-sm-2"></div>
    <div class="col-sm-10">
        {% if comment_status.id is null %}
            <input type="submit" value="Create" class="btn btn-primary" />
            <a href="{{ path('nines_feedback_comment_status_index') }}" class="btn btn-warning"> Cancel </a>
        {% else %}
            <input type="submit" value="Update" class="btn btn-primary" />
            <a href="{{ path('nines_feedback_comment_status_show', {id: comment_status.id}) }}" class="btn btn-warning"> Cancel </a>
        {% endif %}
    </div>
</div>
{{ form_end(form) }}

